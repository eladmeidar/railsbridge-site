<div id="container">
  
  <%= ERB.new(File.read(File.join(RAILS_ROOT,'vendor','plugins','admin_data','lib','views','_header.html.erb'))).result(binding) %>    
  
  <div id="wrapper">
    <div id="main">
      
      <div class="block" id="block-tables">
        <div class="secondary-navigation">
          <div class="clear"></div>
        </div>          
        <div class="content">          
          <h2 class="title">Migration Information from schema_migrations table</h2>
          <div class="inner">
              <table class="table colorize">
                <tr>             
                  <th class='first last'>version</th>
                </tr>
                <% @data.each do |record| %>
                  <tr><td><%=record.values.first%></td></tr>
                <% end %>  
              </table>
          </div>
        </div>
      </div>
        
      <%= ERB.new(File.read(File.join(RAILS_ROOT,'vendor','plugins','admin_data','lib','views','_powered_by.html.erb'))).result(binding) %>      
      
    </div>
    <div class="clear"></div>      
  </div>    
</div>
